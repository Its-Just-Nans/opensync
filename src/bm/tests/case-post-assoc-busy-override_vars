btm_ref_btm_req=$(cat <<-.
	BTM_REQUEST
	mac=20:00:00:00:00:00
	ifname=wlan0
	neighs=
	    bssid=10:00:00:00:00:01
	    bssid_info=143
	    op_class=115
	    phy_type=9
	    opt_subelems=
	    opt_subelems_len=0

	num_neigh=1
	valid_int=255
	abridged=1
	pref=1
	disassoc_imminent=0
	bss_term=0
	tries=0
	max_tries=4
	retry_interval=10
	inc_neigh=false
	inc_self=true
.
)

btm_ref_dpp_client_kick=$(cat <<-.
	mac=20:00:00:00:00:00
	num_band_records=5
	    type=2.4G
	    ifname=wlan0
	    connected=true
	    rejects=0
	    connects=0
	    disconnects=0
	    activity_changes=1
	    steering_success_cnt=0
	    steering_fail_cnt=0
	    steering_kick_cnt=0
	    sticky_kick_cnt=0
	    probe_bcast_cnt=0
	    probe_bcast_blocked=0
	    probe_direct_cnt=0
	    probe_direct_blocked=0
	    num_event_records=5
	    event_record=
	        type=CLIENT_BS_BTM
	        rssi=0
	        probe_bcast=0
	        probe_blocked=0
	        disconnect_src=LOCAL
	        disconnect_type=DISASSOC
	        disconnect_reason=0
	        backoff_enabled=false
	        active=false
	        rejected=false
	        is_BTM_supported=false
	        is_RRM_supported=false
	        band_cap_2G=false
	        band_cap_5G=false
	        max_chwidth=0
	        max_streams=0
