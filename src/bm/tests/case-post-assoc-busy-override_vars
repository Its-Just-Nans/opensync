	        max_txpower=0
	        is_static_smps=false
	        is_mu_mimo_supported=false
	        rrm_caps_link_meas=false
	        rrm_caps_neigh_rpt=false
	        rrm_caps_bcn_rpt_passive=false
	        rrm_caps_bcn_rpt_active=false
	        rrm_caps_bcn_rpt_table=false
