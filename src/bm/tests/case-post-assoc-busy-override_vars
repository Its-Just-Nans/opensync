ref_sta_update_24=$(cat <<-.
	CLIENT_UPDATE
	mac=20:00:00:00:00:00
	if_name=wlan0
	blacklist=false
	rssi_probe_hwm=0
	rssi_probe_lwm=20
	rssi_auth_hwm=0
	rssi_auth_lwm=0
	rssi_inact_xing=0
