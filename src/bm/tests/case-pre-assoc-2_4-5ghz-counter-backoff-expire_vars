#!/bin/sh

ref_update_sta_24_during_backoff=$(cat <<-.
	CLIENT_UPDATE
	mac=20:00:00:00:00:00
	if_name=wlan0
	blacklist=false
	rssi_probe_hwm=0
	rssi_probe_lwm=0
	rssi_auth_hwm=0
	rssi_auth_lwm=0
	rssi_inact_xing=0
	rssi_high_xing=35
	rssi_low_xing=20
	auth_reject_reason=0
.
)

ref_update_sta_5_during_backoff=$(cat <<-.
	CLIENT_UPDATE
	mac=20:00:00:00:00:00
	if_name=wlan1
	blacklist=false
	rssi_probe_hwm=0
	rssi_probe_lwm=0
	rssi_auth_hwm=0
	rssi_auth_lwm=0
	rssi_inact_xing=0
	rssi_high_xing=35
	rssi_low_xing=20
	auth_reject_reason=0
.
)

ref_dpp_client_during_backoff=$(cat <<-.
	mac=20:00:00:00:00:00
	num_band_records=4
	    type=2.4G
	    ifname=wlan0
	    connected=false
	    rejects=7
	    connects=0
	    disconnects=0
	    activity_changes=0
	    steering_success_cnt=0
	    steering_fail_cnt=1
	    steering_kick_cnt=0
	    sticky_kick_cnt=0
	    probe_bcast_cnt=7
	    probe_bcast_blocked=7
	    probe_direct_cnt=0
	    probe_direct_blocked=0
	    num_event_records=3
	    event_record=
	        type=BAND_STEERING_ATTEMPT
	        rssi=0
	        probe_bcast=0
	        probe_blocked=0
	        disconnect_src=LOCAL
	        disconnect_type=DISASSOC
	        disconnect_reason=0
	        backoff_enabled=false
	        active=false
	        rejected=false
	        is_BTM_supported=false
	        is_RRM_supported=false
	        band_cap_2G=false
	        band_cap_5G=false
	        max_chwidth=0
	        max_streams=0
	        phy_mode=0
	        max_MCS=0
	        max_txpower=0
	        is_static_smps=false
	        is_mu_mimo_supported=false
	        rrm_caps_link_meas=false
	        rrm_caps_neigh_rpt=false
	        rrm_caps_bcn_rpt_passive=false
	        rrm_caps_bcn_rpt_active=false
	        rrm_caps_bcn_rpt_table=false
	        rrm_caps_lci_meas=false
	        rrm_caps_ftm_range_rpt=false
	        backoff_period=0
	        assoc_ies=
	        assoc_ies_len=0
	        btm_status=0

	        type=PROBE
	        rssi=50
	        probe_bcast=1
	        probe_blocked=1
	        disconnect_src=LOCAL
	        disconnect_type=DISASSOC
	        disconnect_reason=0
	        backoff_enabled=false
	        active=false
	        rejected=false
	        is_BTM_supported=false
	        is_RRM_supported=false
	        band_cap_2G=false
	        band_cap_5G=false
	        max_chwidth=0
	        max_streams=0
	        phy_mode=0
	        max_MCS=0
	        max_txpower=0
	        is_static_smps=false
	        is_mu_mimo_supported=false
	        rrm_caps_link_meas=false
	        rrm_caps_neigh_rpt=false
	        rrm_caps_bcn_rpt_passive=false
	        rrm_caps_bcn_rpt_active=false
	        rrm_caps_bcn_rpt_table=false
	        rrm_caps_lci_meas=false
	        rrm_caps_ftm_range_rpt=false
	        backoff_period=0
	        assoc_ies=
	        assoc_ies_len=0
	        btm_status=0

	        type=BACKOFF
	        rssi=0
	        probe_bcast=0
	        probe_blocked=0
	        disconnect_src=LOCAL
	        disconnect_type=DISASSOC
	        disconnect_reason=0
	        backoff_enabled=true
	        active=false
	        rejected=false
	        is_BTM_supported=false
	        is_RRM_supported=false
	        band_cap_2G=false
	        band_cap_5G=false
	        max_chwidth=0
	        max_streams=0
	        phy_mode=0
	        max_MCS=0
	        max_txpower=0
	        is_static_smps=false
	        is_mu_mimo_supported=false
	        rrm_caps_link_meas=false
	        rrm_caps_neigh_rpt=false
	        rrm_caps_bcn_rpt_passive=false
	        rrm_caps_bcn_rpt_active=false
	        rrm_caps_bcn_rpt_table=false
	        rrm_caps_lci_meas=false
	        rrm_caps_ftm_range_rpt=false
	        backoff_period=120
	        assoc_ies=
	        assoc_ies_len=0
	        btm_status=0

	    type=5GL
	    ifname=wlan1
	    connected=false
	    rejects=0
	    connects=0
	    disconnects=0
	    activity_changes=0
	    steering_success_cnt=0
	    steering_fail_cnt=0
	    steering_kick_cnt=0
	    sticky_kick_cnt=0
	    probe_bcast_cnt=2
	    probe_bcast_blocked=0
	    probe_direct_cnt=0
	    probe_direct_blocked=0
	    num_event_records=1
	    event_record=
	        type=PROBE
	        rssi=50
	        probe_bcast=1
	        probe_blocked=0
	        disconnect_src=LOCAL
	        disconnect_type=DISASSOC
	        disconnect_reason=0
	        backoff_enabled=false
	        active=false
	        rejected=false
	        is_BTM_supported=false
	        is_RRM_supported=false
	        band_cap_2G=false
	        band_cap_5G=false
	        max_chwidth=0
	        max_streams=0
	        phy_mode=0
	        max_MCS=0
	        max_txpower=0
	        is_static_smps=false
	        is_mu_mimo_supported=false
	        rrm_caps_link_meas=false
	        rrm_caps_neigh_rpt=false
	        rrm_caps_bcn_rpt_passive=false
	        rrm_caps_bcn_rpt_active=false
	        rrm_caps_bcn_rpt_table=false
	        rrm_caps_lci_meas=false
	        rrm_caps_ftm_range_rpt=false
	        backoff_period=0
	        assoc_ies=
	        assoc_ies_len=0
	        btm_status=0
.
)

ref_update_sta_24_after_backoff_expire=$(cat <<-.
	CLIENT_UPDATE
	mac=20:00:00:00:00:00
	if_name=wlan0
	blacklist=false
	rssi_probe_hwm=1
	rssi_probe_lwm=20
	rssi_auth_hwm=1
	rssi_auth_lwm=0
	rssi_inact_xing=0
	rssi_high_xing=35
	rssi_low_xing=20
	auth_reject_reason=0
.
)

ref_update_sta_5_after_backoff_expire=$(cat <<-.
	CLIENT_UPDATE
